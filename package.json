{
  "name": "apod-slack-lambda",
  "version": "1.0.2",
  "description": "The Astronomy Picture of the Day via Slack, with a little help from AWS Lambda",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "prettier": "prettier --write \"**/*.{js,md,json,yml}\"",
    "prettier-check": "prettier-check \"**/*.{js,md}\"",
    "test": "npm run lint && npm run prettier-check"
  },
  "files": [
    "**/*.js",
    "/config",
    "/apod-to-slack",
    "!dist"
  ],
  "repository": "paulmelnikow/apod-slack-lambda",
  "keywords": [
    "apod",
    "slack",
    "aws",
    "lambda"
  ],
  "author": "Paul Melnikow",
  "license": "MIT",
  "devDependencies": {
    "aws-sdk": "^2.16.0",
    "config": "^3.0.0",
    "del": "^5.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "gulp": "^4.0.0",
    "gulp-install": "^1.1.0",
    "gulp-zip": "^5.0.0",
    "js-yaml": "^3.6.1",
    "node-aws-lambda": "^0.1.8",
    "node-aws-lambda-scheduler": "^1.0.4",
    "prettier": "^1.15.2",
    "prettier-check": "^2.0.0"
  },
  "dependencies": {
    "nasa-apod": "^0.2.0",
    "pify": "^4.0.1",
    "slack-node": "^0.2.0",
    "underscore": "^1.8.3"
